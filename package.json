{
  "name": "tutorwise",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:backend": "cd tutorwise-railway-backend && python3 -m pytest tests/ -v",
    "test:backend:coverage": "cd tutorwise-railway-backend && python3 -m pytest tests/ -v --cov=app --cov-report=term-missing --cov-report=html",
    "test:all": "npm run test:backend && npm run test && npm run test:e2e",
    "lint:backend": "cd tutorwise-railway-backend && python3 -m ruff check app/",
    "lint:backend:fix": "cd tutorwise-railway-backend && python3 -m ruff check --fix app/",
    "lint:all": "npm run lint && npm run lint:backend",
    "quality:check": "npm run lint:all && npm run test:all",
    "health:check": "cd tutorwise-railway-backend && python3 app/backend_healthcheck.py",
    "sync:jira": "node .ai/integrations/sync-jira.js",
    "sync:github": "node .ai/integrations/sync-github.js",
    "sync:context": "npm run sync:jira && npm run sync:github",
    "dev:with-context": "npm run sync:context && npm run dev"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^3.5.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.51.0",
    "@vercel/blob": "^1.1.1",
    "@vercel/speed-insights": "^1.2.0",
    "date-fns": "^4.1.0",
    "encoding": "^0.1.13",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "next": "^14.2.32",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.32",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "stylelint": "^16.22.0",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "undici": "^7.16.0",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
